{
  "name": "aws-bedrock-multiagents",
  "private": true,
  "packageManager": "pnpm@10.30.3+sha512.c961d1e0a2d8e354ecaa5166b822516668b7f44cb5bd95122d590dd81922f606f5473b6d23ec4a5be05e7fcd18e8488d47d978bbe981872f1145d06e9a740017",
  "scripts": {
    "build": "pnpm -r build",
    "build:shared": "pnpm --filter @aws-bedrock-multiagents/shared run build",
    "build:lambdas": "pnpm --filter @aws-bedrock-multiagents/tool-work-search run build && pnpm --filter @aws-bedrock-multiagents/tool-rag-search run build",
    "build:infra": "pnpm --filter @aws-bedrock-multiagents/infra-cdk run build",
    "typecheck": "pnpm -r typecheck",
    "typecheck:tsc": "pnpm -r typecheck:tsc",
    "test:agent": "tsx scripts/test-agent.ts",
    "eval:agent": "tsx scripts/eval-agent.ts",
    "clean": "pnpm -r exec rm -rf dist cdk.out",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "oxlint --tsconfig tsconfig.base.json .",
    "lint:fix": "oxlint --tsconfig tsconfig.base.json --fix .",
    "lint:staged": "lint-staged",
    "check": "pnpm typecheck && pnpm lint && pnpm format:check",
    "prepare": "husky",
    "presynth": "pnpm build",
    "synth": "pnpm --filter @aws-bedrock-multiagents/infra-cdk synth",
    "prediff": "pnpm build",
    "diff": "pnpm --filter @aws-bedrock-multiagents/infra-cdk diff",
    "predeploy": "pnpm build",
    "deploy": "pnpm --filter @aws-bedrock-multiagents/infra-cdk run deploy",
    "precdk:deploy": "pnpm build",
    "cdk:deploy": "pnpm --filter @aws-bedrock-multiagents/infra-cdk run deploy",
    "bootstrap": "pnpm --filter @aws-bedrock-multiagents/infra-cdk bootstrap",
    "destroy": "pnpm --filter @aws-bedrock-multiagents/infra-cdk exec cdk destroy BedrockAgentsStack --force"
  },
  "devDependencies": {
    "@aws-sdk/client-bedrock-agent-runtime": "^3.884.0",
    "@types/node": "^22.13.10",
    "chalk": "^5.4.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "oxlint": "^1.14.0",
    "prettier": "^3.5.3",
    "tsx": "^4.20.5",
    "typescript": "^5.7.3"
  }
}
